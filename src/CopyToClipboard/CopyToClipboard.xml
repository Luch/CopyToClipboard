<?xml version="1.0" encoding="utf-8" ?>
<widget id="CopyToClipboard.widget.CopyToClipboard" needsEntityContext="false" xmlns="http://www.mendix.com/widget/1.0/">
  <name>CopyToClipboard</name>
  <description>CopyToClipboard for Mendix.</description>

  <icon></icon>

  <properties>
 		<property key="elementnameString" type="string" required="true" >
            <caption>ElementName</caption>
            <category>Setup</category>
            <description>Name of the Element to copy value from.</description>
        </property>
		
        <property key="buttoncaption" type="string" required="false" defaultValue="Copy to clipboard">
            <caption>Caption</caption>
            <category>General</category>
            <description>The caption to show on button</description>
        </property>

        <property key="buttonimage" type="image" required="false" >
            <caption>Image</caption>
            <category>General</category>
            <description>The image to show on button</description>
        </property>

        <property key="buttontooltip" type="string" required="false" >
            <caption>Tooltip</caption>
            <category>General</category>
            <description>The tooltip to show on button hover</description>
        </property>

        <property key="buttonclass" type="string" required="false" >
            <caption>Button Class</caption>
            <category>General</category>
            <description>Class</description>
        </property>

        <property key="buttonstyle" type="string" required="false" >
            <caption>Button Style</caption>
            <category>General</category>
            <description>Style</description>
        </property>

  </properties>
</widget>

